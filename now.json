{
  "version": 2,
  "builds": [
    {
      "src": "src/main.jsx",
      "use": "@now/node"
    },
    {
      "src": "src/routes/route-hotel.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.jsx"
    }
  ],
  "public": {
    "ignore": ["**/*.jsx"]
  }
}
